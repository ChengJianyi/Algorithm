#include <iostream>
#include <vector>
#include <Eigen/Eigen>

using namespace std;
using namespace Eigen;

class Sample{
    private:
        VectorXd vec;
        int label;
    public:
        Sample();
        const& VectorXd getVec();
        const& int getLabel();
        void setVec(const& VectorXd v);
        void setLabel(const& int l);
};
